@page "/"
@namespace FreeloadMap.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FreeloadMap</title>

    <base href="~/" />

    <!-- freeloadmap/ -->
    <link href="css/bootstrap/bootstrap.min.css" rel="stylesheet" />

    <!-- freeloadmap/ -->
    <link href="css/site.css" rel="stylesheet" />
    <!-- freeloadmap/ -->
    <link href="css/mycss.css" rel="stylesheet" />

    <!-- https://github.com/BlazorExtensions/Canvas#usage -->
    <!-- Blazor.Extensions.Canvas -->
    <script src="_content/Blazor.Extensions.Canvas/blazor.extensions.canvas.js" />

    <link href="images/" rel="contents"></script>
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <!-- freeloadmap/ -->
    <script src="_framework/blazor.server.js"></script>
    <script src="js/jquery/jquery-3.6.0.min.js"></script>
    <script src="js/myjs.js"></script>
</body>
</html>
